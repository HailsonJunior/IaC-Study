{
    "variables": {
        "ami_id": "ami-09e67e426f25ce0d7",
        "aws_region": "us-east-1",
        "ssh-username": "ubuntu",
        "aws_vpc_id": "vpc-60245c1d"
    },

    "builders": [{
        "type": "amazon-ebs",
        "region": "{{user `aws_region`}}",
        "source_ami": "{{user `ami_id`}}",
        "instance_type": "t2.micro",
        "ssh_username": "ubuntu",
        "ami_name": "AWS-Study",
        "ssh-keypair_name": "iac-new",
        "ssh_private_key_file": "iac-new.pem",
        "vpc_id": "{{user `vpc_id`}}"
    }],

    "provisioners": [
        {
            "type": "ansible",
            "playbook_file": "./provisioners/ansible/setup-machine.yml",
            "user": "{{user `ssh_username`}}",
            "ansible_env_vars": [
                "ANSIBLE_HOST_KEY_CHECKING=False"
            ]
        }
    ]
}